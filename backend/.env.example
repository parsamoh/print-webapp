# Server Configuration
PORT=3000

# CUPS Configuration
# For local CUPS: http://localhost:631
# For network CUPS: http://YOUR_ROUTER_IP:631
CUPS_URL=http://localhost:631

# The printer name as shown in CUPS (use 'lpstat -p' to list printers)
CUPS_PRINTER_NAME=your_printer_name

# Upload and Temporary Files
UPLOAD_DIR=../uploads
TEMP_DIR=../temp

# Cleanup temporary files after (in milliseconds, default 1 hour)
TEMP_FILE_CLEANUP=3600000
